# Configuración de Google Analytics 4
GA4_PROPERTY_ID=tu_property_id_aqui

# Zona horaria (importante para consistencia con GA4)
# Ejemplos comunes:
# America/Mexico_City (GMT-6)
# America/New_York (GMT-5)
# Europe/Madrid (GMT+1)
# America/Bogota (GMT-5)
GA4_TIMEZONE=America/Mexico_City

# Configuración adicional
# Días hacia atrás para consultar (por defecto: 1 = ayer)
DAYS_BACK=1

# Activar logging detallado (true/false)
DEBUG_MODE=true